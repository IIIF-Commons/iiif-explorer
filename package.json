{
  "name": "iiif-explorer",
  "version": "0.0.12",
  "description": "IIIF Explorer Component",
  "main": "dist/iiifexplorer.js",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "browser": "dist/iiifexplorer.js",
  "scripts": {
    "build": "stencil build",
    "dev": "sd concurrent \"stencil build --dev --prod --watch\" \"stencil-dev-server\" ",
    "serve": "stencil-dev-server",
    "start": "npm run dev",
    "test": "jest --no-cache",
    "test.watch": "jest --watch --no-cache"
  },
  "dependencies": {
    "@stencil/core": "^0.7.27-4",
    "manifesto.js": "^2.2.12"
  },
  "devDependencies": {
    "@stencil/dev-server": "latest",
    "@stencil/utils": "latest",
    "@types/jest": "^21.1.1",
    "classnames": "^2.2.5",
    "jest": "^21.2.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iiif-commons/iiif-explorer.git"
  },
  "author": "@edsilv",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iiif-commons/iiif-explorer"
  },
  "homepage": "https://github.com/iiif-commons/iiif-explorer",
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  },
  "keywords": [
    "iiif",
    "gallery",
    "webcomponent"
  ]
}
